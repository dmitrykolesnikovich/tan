fn assert(a: u32) : void;

fn main(argc: int, argv: char**) : int {
    @assert(13 / 3 == 4);
    @assert(1.0 / 2.0 == 0.5);
    @assert(55 / 11 == 5);
    @assert(-55 / 11 == -5);
    @assert(55.0 / 11.0 == 5.0);
    @assert(-55.0 / 11.0 == -5.0);
    @assert(55.0 / 11.0 == 5.0);
    @assert(-55.0 / 11.0 == -5.0);
    return 0;
}
