import "runtime/print.tan";
import "runtime/debug.tan";

struct Fuck;
struct Useless;

struct Useless {
    var shit: i8 = 1;
    var f: float;
    var fuck: Fuck;
}

struct Fuck {
    var val: i32 = 2;
}

pub fn main(argc: int, argv: u8**) : i32 {
    var s: Useless;
    var f: Fuck;
    s.fuck = f;
    assert(s.shit == 1);
    assert(s.fuck.val == 2);
    s.shit = 100;
    s.f = 1.0;
    s.fuck.val = 11;
    assert(s.fuck.val == 11);
    var shit: i8 = s.shit;
    assert(s.shit == 100);
    assert(shit == 100);
    assert(s.f == 1.0);

    print("SUCCESS\n");
    return 0;
}
