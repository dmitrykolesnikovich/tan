fn print(s: str) : int;

// TODO: fix return inside if statement
fn sign(a: int) : int {
    var ret: int = 0;
    if (a > 0) {
        ret = 1;
    } else if (a < 0) {
        ret = -1;
    }
    return ret;
}

fn else_if(c: i32) : i32 {
    var ret: i32 = -100;
    if (c == 0) {
        ret = 0;
    } else if (c == 1) {
        ret = 1;
    } else {
        ret = 2;
    }
    return ret;
}

fn main(argc: int, argv: char**) : int {
    if (argc > 0) {
        @assert(1);
    } else {
        @assert(0);
    }
    @assert(else_if(1) == 1);
    print("SUCCESS");
    return 0;
}
