extern fn print(s: str) : int;

pub fn main(argc: int, argv: char**) : int {
    @assert(!0.0);
    @assert(!1.0 == 0);
    @assert(1 < 2);
    @assert(1.1 < 2.2);
    @assert(1.0 + 2.0 > 2.8);
    @assert(3.0 > 2);
    @assert(1 > -2.0);
    @assert(1 != 0);
    @assert(!(1 == 0));
    @assert(1.0 + 2.0 == 3);
    @assert(1 + 3 == 2 + 2);
    @assert(1.0 + 3.0 == 2 + 2.0);
    print("SUCCESS\n");
    return 0;
}
